<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:baseMessagesInterface="edu.stanford.bmir.protege.web.client.Messages"
             ui:generateFormat="com.google.gwt.i18n.server.PropertyCatalogFactory"
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="en">

    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>

    <g:HTMLPanel>

        <table width="95%">
            <col width="100px" align="right"/>
            <tr>
                <td align="right">
                    <g:Label styleName="{wp.style.formLabel}" addStyleNames="web-protege-form-layout-editor-input">
                      <ui:msg>Ontology IRI</ui:msg>
                    </g:Label>
                </td>
                <td valign="top">
                    <g:TextBox ui:field="ontologyIRIField" width="100%" addStyleNames="web-protege-form-layout-editor-input"/>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <g:Label addStyleNames="{wp.style.formLabel} web-protege-form-layout-editor-input">
                      <ui:msg>Version IRI</ui:msg>
                    </g:Label>
                </td>
                <td valign="top">
                    <g:TextBox ui:field="versionIRIField" width="100%" addStyleNames="web-protege-form-layout-editor-input"/>
                </td>
            </tr>
        </table>
    </g:HTMLPanel>
</ui:UiBinder>
