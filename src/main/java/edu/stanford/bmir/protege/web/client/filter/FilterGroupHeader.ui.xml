<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:baseMessagesInterface="edu.stanford.bmir.protege.web.client.Messages"
             ui:generateFormat="com.google.gwt.i18n.server.PropertyCatalogFactory"
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="en">

    <ui:style>
        .main {
            margin-bottom: 5px;
            white-space: nowrap;
        }
        .label {
            font-weight: bold;
        }
    </ui:style>
    <g:HTMLPanel addStyleNames="{style.main}">
        <g:InlineLabel ui:field="label" addStyleNames="{style.label}"/>  <g:Anchor ui:field="selectAllButton"><ui:msg>Select All</ui:msg></g:Anchor> | <g:Anchor ui:field="selectNoneButton"><ui:msg>Select None</ui:msg></g:Anchor>
    </g:HTMLPanel>
</ui:UiBinder>
