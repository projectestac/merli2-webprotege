<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:baseMessagesInterface="edu.stanford.bmir.protege.web.client.Messages"
             ui:generateFormat="com.google.gwt.i18n.server.PropertyCatalogFactory"
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="en">

    <ui:style>
        .main {
            width: 500px;
            margin-left: auto;
            margin-right: auto;
            text-align: center;
            line-height: 40px;
            font-size: 16px;
            margin-top: 30px;
            color: #606060;
            font-weight: 200;
        }
        .idLabel {
            font-size: smaller;
        }
    </ui:style>

    <g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{style.main}">
            <div>
                <ui:msg>Could not create view.</ui:msg>
            </div>
            <div>
                <ui:msg>The view specified in the configuration could not be found.</ui:msg>
            </div>
            <div class="{style.idLabel}">
                <ui:msg>Specified view:</ui:msg> <g:InlineLabel ui:field="portletIdField"/>
            </div>


        </g:HTMLPanel>

    </g:HTMLPanel>
</ui:UiBinder>
