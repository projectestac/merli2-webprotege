<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:baseMessagesInterface="edu.stanford.bmir.protege.web.client.Messages"
             ui:generateFormat="com.google.gwt.i18n.server.PropertyCatalogFactory"
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="en">
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <ui:style>
        .main-panel {
            padding: 20px;
        }
    </ui:style>
    <g:HTMLPanel addStyleNames="{style.main-panel}">
        <g:InlineLabel addStyleNames="{wp.style.formLabel}">
          <ui:msg>Download format: </ui:msg>
        </g:InlineLabel>
        <g:ListBox ui:field="formatListBox" visibleItemCount="1"/>
    </g:HTMLPanel>
</ui:UiBinder>
